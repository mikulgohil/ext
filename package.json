{
  "name": "nextjs-component-generator",
  "displayName": "Next.js Component Generator",
  "description": "Generate Next.js components using OpenAI with text and image input",
  "version": "1.0.0",
  "publisher": "nextjs-component-generator",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "nextjs",
    "react",
    "component",
    "generator",
    "openai",
    "gpt",
    "ai",
    "tailwind"
  ],
  "activationEvents": [
    "onCommand:nextjs-component-generator.start"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nextjs-component-generator.start",
        "title": "Next.js Component Generator: Start Chat"
      }
    ],
    "configuration": {
      "title": "Next.js Component Generator",
      "properties": {
        "nextjsComponentGenerator.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "Your OpenAI API key for generating components"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "prepackage": "npm run build",
    "package": "vsce package",
    "prepublish": "npm run build",
    "publish": "vsce publish",
    "watch": "tsc -watch -p ./",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "webpack-prod": "webpack --mode production",
    "build": "npm run webpack-prod"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "openai": "^4.20.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.2",
    "ajv": "^8.12.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "vsce": "^2.15.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nextjs-component-generator.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/nextjs-component-generator/issues"
  },
  "homepage": "https://github.com/yourusername/nextjs-component-generator#readme",
  "license": "MIT"
}
